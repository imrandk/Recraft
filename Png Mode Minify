const div=document.querySelector(".c-iKhdKS"),widthInput=document.querySelector('span[data-testid="width-input-area"]'),heightInput=document.querySelector('span[data-testid="height-input-area"]');function reloadPage(){window.location.reload()}function adjustSizes(){const t=setInterval((()=>{widthInput&&heightInput&&(widthInput.innerText="3840",widthInput.focus(),widthInput.dispatchEvent(new Event("input",{bubbles:!0})),heightInput.innerText="2160",heightInput.focus(),heightInput.dispatchEvent(new Event("input",{bubbles:!0})),clearInterval(t),performClicks())}),100)}function performClicks(){div.click();const t=setInterval((()=>{const e=document.querySelector('.c-leHWbT:nth-child(1) > [data-testid="toggler"]'),n=document.querySelector(".c-dENsEo-ifsGJtF-css > .c-bZNrxE");e&&n&&(e.click(),n.click(),clearInterval(t),setTimeout(reloadPage,1e3))}),100)}adjustSizes();
