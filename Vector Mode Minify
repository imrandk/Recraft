const div=document.querySelector(".c-iKhdKS"),widthInput=document.querySelector('span[data-testid="width-input-area"]'),heightInput=document.querySelector('span[data-testid="height-input-area"]');function reloadPage(){window.location.reload()}function performClicks(){div.click();const t=setInterval((()=>{const e=document.querySelector(".c-dENsEo-ichpEri-css > .c-bZNrxE");(e||attempts++>=50)&&(e?.click(),clearInterval(t),adjustSizes())}),100)}function adjustSizes(){const t=setInterval((()=>{(widthInput&&heightInput||sizeAttempts++>=50)&&(widthInput&&heightInput&&(widthInput.innerText="3840",widthInput.focus(),widthInput.dispatchEvent(new Event("input",{bubbles:!0})),heightInput.innerText="2160",heightInput.focus(),heightInput.dispatchEvent(new Event("input",{bubbles:!0}))),clearInterval(t),setTimeout(reloadPage,1e3))}),100)}performClicks();
